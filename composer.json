{
  "name": "dejwcake/php-code-style",
  "description": "Code style for php applications and libraries based on Slevomat Rules.",
  "license": "MIT",
  "require": {
    "php": "^8.0 || ^8.1 || ^8.2",
    "slevomat/coding-standard": "8.11.1"
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 3600,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
